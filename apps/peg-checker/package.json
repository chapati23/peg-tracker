{
  "name": "peg-checker",
  "version": "1.0.0",
  "description": "",
  "author": "Philip Paetz <philip.paetz@me.com>",
  "license": "UNLICENSED",
  "main": "dist/index.js",
  "type": "module",
  "packageManager": "npm@9.4.2",
  "scripts": {
    "build": "npx is-ci || tsc",
    "build:watch": "tsc --watch",
    "clean": "rimraf dist node_modules && rimraf *.tgz || true",
    "deploy": "./deploy.sh",
    "dev": "npx nodemon --watch dist --watch '../../packages/**/dist/**/*' --exec 'npm start'",
    "lint": "eslint src --fix --report-unused-disable-directives",
    "scriptlint": "npx scriptlint",
    "start": "DEBUG=peg-checker,alerts,curve,user DEBUG_COLORS=true env-cmd functions-framework --target pegChecker --port 8081",
    "start:prod": "NODE_ENV=production env-cmd functions-framework --target pegChecker",
    "test": "echo '❗️ Please add tests soon ❗️'",
    "trigger": "./trigger.local.sh",
    "trigger:prod": "./trigger.prod.sh"
  },
  "overrides": {
    "cloudevents": "^7.0.0"
  },
  "dependencies": {
    "@google-cloud/firestore": "^6.5.0",
    "@google-cloud/functions-framework": "^3.2.0",
    "alerts": "^1.0.0",
    "bot": "*",
    "curve": "*",
    "debug": "^4.3.4",
    "telegram": "*",
    "user": "*"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.5",
    "eslint-config-custom": "*",
    "is-ci": "^3.0.1",
    "rimraf": "^5.0.1",
    "tsconfig": "*"
  }
}
